{
  "apiVersion": "v1",
  "kind": "List",
  "items": [
    {
      "apiVersion": "apps.openshift.io/v1",
      "kind": "DeploymentConfig",
      "metadata": {
        "name": "react-app",
        "namespace": "demo-entel",
        "labels": {
          "project": "react-app",
          "version": "0.1.0",
          "provider": "nodeshift",
          "app": "react-app",
          "app.kubernetes.io/name": "nodejs",
          "app.kubernetes.io/component": "react-app",
          "app.kubernetes.io/instance": "react-app",
          "app.openshift.io/runtime": "nodejs"
        },
        "annotations": {
          "nodeshift/git-branch": "main"
        }
      },
      "spec": {
        "replicas": 1,
        "revisionHistoryLimit": 2,
        "selector": {
          "app": "react-app",
          "project": "react-app",
          "provider": "nodeshift"
        },
        "triggers": [
          {
            "type": "ConfigChange"
          },
          {
            "type": "ImageChange",
            "imageChangeParams": {
              "automatic": true,
              "containerNames": [
                "react-app"
              ],
              "from": {
                "kind": "ImageStreamTag",
                "namespace": "demo-entel",
                "name": "react-app:latest"
              }
            }
          }
        ],
        "template": {
          "metadata": {
            "labels": {
              "project": "react-app",
              "version": "0.1.0",
              "provider": "nodeshift",
              "app": "react-app"
            },
            "annotations": {
              "nodeshift/git-branch": "main"
            }
          },
          "spec": {
            "containers": [
              {
                "image": "react-app",
                "name": "react-app",
                "securityContext": {
                  "privileged": false
                },
                "ports": [
                  {
                    "containerPort": 8080,
                    "name": "http",
                    "protocol": "TCP"
                  }
                ],
                "env": []
              }
            ]
          }
        }
      }
    },
    {
      "apiVersion": "v1",
      "kind": "Service",
      "spec": {
        "type": "ClusterIP",
        "selector": {
          "project": "react-app",
          "provider": "nodeshift"
        },
        "ports": [
          {
            "protocol": "TCP",
            "port": 8080,
            "targetPort": 8080,
            "name": "http"
          }
        ]
      },
      "metadata": {
        "name": "react-app",
        "namespace": "demo-entel",
        "labels": {
          "project": "react-app",
          "version": "0.1.0",
          "provider": "nodeshift",
          "app.kubernetes.io/name": "nodejs",
          "app.kubernetes.io/component": "react-app",
          "app.kubernetes.io/instance": "react-app",
          "app.openshift.io/runtime": "nodejs"
        },
        "annotations": {
          "nodeshift/git-branch": "main"
        }
      }
    },
    {
      "apiVersion": "route.openshift.io/v1",
      "kind": "Route",
      "spec": {
        "to": {
          "kind": "Service",
          "name": "react-app"
        },
        "tls": {
          "insecureEdgeTerminationPolicy": "Redirect"
        },
        "port": {
          "targetPort": 8080
        }
      },
      "metadata": {
        "name": "react-app",
        "labels": {
          "project": "react-app",
          "version": "0.1.0",
          "provider": "nodeshift",
          "app.kubernetes.io/name": "nodejs",
          "app.kubernetes.io/component": "react-app",
          "app.kubernetes.io/instance": "react-app",
          "app.openshift.io/runtime": "nodejs"
        }
      }
    }
  ]
}